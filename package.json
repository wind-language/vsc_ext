{
  "name": "wind-lang",
  "displayName": "Wind Language",
  "description": "Wind Language Syntax",
  "version": "0.0.1",
  "publisher": "WindLang",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "wind-lang",
      "aliases": ["Wind", "wind-lang"],
      "extensions": [".w .wi"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "wind-lang",
      "scopeName": "source.wind",
      "path": "./syntaxes/wind-lang.tmLanguage.json"
    }]
  },
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/wind-language/vsc_ext"
  }
}
